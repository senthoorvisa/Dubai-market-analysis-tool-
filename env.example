# Dubai Market Analysis Tool - Environment Variables

# AI Services
GEMINI_API_KEY=your_gemini_api_key_here

# Database Configuration
MONGODB_URI=mongodb://localhost:27017/dubai-market-analysis
REDIS_URL=redis://localhost:6379

# DLD API Configuration
DLD_API_KEY=your_dld_api_key_here
DLD_API_SECRET=your_dld_api_secret_here
DLD_BASE_URL=https://api.dld.gov.ae

# External APIs
DUBAI_STATISTICS_API_KEY=your_dubai_stats_api_key_here
WORLD_BANK_API_KEY=your_world_bank_api_key_here

# Logging and Monitoring
SENTRY_DSN=your_sentry_dsn_here
LOG_LEVEL=info

# Vector Database (Pinecone)
PINECONE_API_KEY=your_pinecone_api_key_here
PINECONE_ENVIRONMENT=your_pinecone_environment_here
PINECONE_INDEX_NAME=dubai-market-analysis

# Server Configuration
PORT=3001
NODE_ENV=development

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Cache Configuration
CACHE_TTL_DEFAULT=3600
CACHE_TTL_LONG=86400

# Scheduler Configuration
ENABLE_SCHEDULERS=true
SCHEDULER_TIMEZONE=Asia/Dubai

# Notification Services (Optional)
SLACK_WEBHOOK_URL=your_slack_webhook_url_here
EMAIL_SERVICE_API_KEY=your_email_service_api_key_here 